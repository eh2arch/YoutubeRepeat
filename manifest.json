{
  "name": "YoutubeRepeat",
  "description": "Loop youtube videos indifintely without loading again",
  "version": "0.1",
  "manifest_version": 2,
  "permissions": ["tabs", "http://*/"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["font-awesome-4.4.0/css/font-awesome.min.css", "custom.css"],
      "js": ["jquery.min.js", "background.js"]
    }
  ],
  "web_accessible_resources":[
    "font-awesome-4.4.0/fonts/*"
  ],
  "icons": { "256": "YTRepeat.png" }
}
